<%= form_for @note,:url=>admin_notes_path, :html => {:class => 'form-horizontal'} do |f| %>

    <div class="control-group">
      <%= f.label :book_name, :class => 'control-label' %>
      <div class="controls">
       <%= f.text_field :book_name, :class => 'text_field'%>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :grade, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :grade, :class => 'text_field'%>

      </div>
    </div>
    <div class="control-group">
      <%= f.label :chapter_name, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :chapter_name, :class => 'text_field'%>

      </div>
    </div>
    <div class="control-group">
      <%= f.label :file, "Upload Notes".html_safe, :class => 'control-label' %>
      <div class="controls">
        <%= f.file_field :file %>
      </div>
    </div>

    <div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                  topics_path, :class => 'btn' %>
    </div>
 <% end %>